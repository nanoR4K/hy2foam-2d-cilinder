FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      hTCProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

hTC2Model  laminar2<rho2ChemistryHTC>;

active     on;

laminar2Coeffs
{}

mixtureOutputs
{
    molarFraction       off;
    numberDensity       on;
    partialDensity      off;
    partialPressure     off;
    degreesOfFreedom    off;
    vibrationalEnergy   off;
}

